{
    "name": "refactor-ranger",
    "version": "1.0.0",
    "private": true,
    "description": "An AI code Review App",
    "author": "Ve Varun Sharma",
    "license": "ISC",
    "homepage": "https://github.com//",
    "keywords": [
        "probot",
        "github",
        "probot-app"
    ],
    "scripts": {
        "build": "tsc --build",
        "start": "probot run ./dist/index.js",
        "dev": "npm run build && npm run start",
        "test": "jest --config ./src/config/jest/jest.config.js --setupFiles dotenv/config --coverage --verbose --testTimeout=10000",
        "lint": "eslint",
        "prettier-format": "prettier --write \"src/**/*.ts\"",
        "prettier-check": "prettier --check \"src/**/*.ts\""
    },
    "dependencies": {
        "@google/generative-ai": "^0.21.0",
        "dotenv": "^16.4.7",
        "probot": "^13.0.1"
    },
    "devDependencies": {
        "@eslint/js": "^8.57.1",
        "@types/jest": "^29.5.14",
        "@types/node": "^20.0.0",
        "eslint": "^8.57.1",
        "globals": "^15.13.0",
        "jest": "^29.7.0",
        "nock": "^14.0.0-beta.5",
        "prettier": "^3.4.2",
        "smee-client": "^2.0.0",
        "ts-jest": "^29.2.5",
        "typescript": "^5.3.3",
        "typescript-eslint": "^7.2.0",
        "vitest": "^1.3.1"
    },
    "engines": {
        "node": ">= 18"
    },
    "type": "commonjs"
}
